  <name>dmmaTensorCoreGemm</name>
  <cflags>
    <flag>--std=c++11</flag>
  </cflags>
  <cuda_api_list>
    <toolkit>cudaMemset</toolkit>
    <toolkit>cudaFree</toolkit>
